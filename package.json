{
    "name": "elo-agent",
    "productName": "elo Agent",
    "version": "1.3.0",
    "main": "dist-electron/main/index.js",
    "description": "elo Agent for identifying (un-)used JAR libraries",
    "author": "Carsten Rambow",
    "license": "Apache-2.0",
    "funding": {
        "type": "buyMeACoffee",
        "url": "https://buymeacoffee.com/elomagic"
    },
    "private": true,
    "debug": {
        "env": {
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
        }
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "npm run licenses && tsc && vite build && electron-builder",
        "lint": "eslint .",
        "preview": "vite preview",
        "pretest": "vite build --mode=test",
        "test": "vitest run",
        "licenses": "license-report --only=prod --json > ./src/assets/license-report.json"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@fontsource/roboto": "^5.2.6",
        "@mui/icons-material": "^7.2.0",
        "@mui/material": "^7.2.0",
        "@mui/x-data-grid": "^8.6.0",
        "@testing-library/react": "^16.3.0",
        "dot-properties": "^1.1.0",
        "electron-log": "^5.4.1",
        "electron-updater": "^6.6.2",
        "jszip": "^3.10.1",
        "react-icons": "^5.5.0",
        "react-toastify": "^11.0.5"
    },
    "devDependencies": {
        "@playwright/test": "^1.53.2",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^5.0.2",
        "autoprefixer": "^10.4.21",
        "chokidar": "^4.0.3",
        "electron": "^37.1.0",
        "electron-builder": "^26.0.12",
        "license-report": "^6.8.0",
        "postcss": "^8.5.6",
        "postcss-import": "^16.1.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "typescript": "^5.8.3",
        "vite": "^7.0.0",
        "vite-plugin-electron": "^0.29.0",
        "vite-plugin-electron-renderer": "^0.14.6",
        "vitest": "^3.2.4"
    }
}
